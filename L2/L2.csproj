<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>disable</Nullable>
    <AssemblyName>laney</AssemblyName>
    <RootNamespace>ELOR.Laney</RootNamespace>
    <AssemblyVersion>2.0.582</AssemblyVersion>
    <FileVersion>2.0.582</FileVersion>
    <Version Condition="$([MSBuild]::IsOSPlatform('Windows'))">2.0.582-win-anycpu-devuser.devpc-240311-0637</Version>
    <Version Condition="$([MSBuild]::IsOSPlatform('Linux'))">2.0.582-linux-anycpu-devuser.devpc-240311-0637</Version>
    <Version Condition="$([MSBuild]::IsOSPlatform('OSX'))">2.0.582-macos-anycpu-devuser.devpc-240311-0637</Version>
    <DefineConstants Condition="$([MSBuild]::IsOSPlatform('Windows'))">WIN</DefineConstants>
    <DefineConstants Condition="$([MSBuild]::IsOSPlatform('Linux'))">LINUX</DefineConstants>
    <DefineConstants Condition="$([MSBuild]::IsOSPlatform('OSX'))">MAC</DefineConstants>
    <Platforms>AnyCPU</Platforms>
    <!--Avalonia doesen't support TrimMode=link currently,but we are working on that https://github.com/AvaloniaUI/Avalonia/issues/6892 -->
    <TrimMode>copyused</TrimMode>
    <AvaloniaUseCompiledBindingsByDefault>true</AvaloniaUseCompiledBindingsByDefault>
  </PropertyGroup>
  <PropertyGroup>
    <CFBundleName>Laney</CFBundleName>
    <CFBundleDisplayName>Laney</CFBundleDisplayName>
    <CFBundleIdentifier>top.elor.laney</CFBundleIdentifier>
    <CFBundleVersion>2.0.582</CFBundleVersion>
    <CFBundleShortVersionString>2.0.582</CFBundleShortVersionString>
    <CFBundlePackageType>APPL</CFBundlePackageType>
    <CFBundleSignature>????</CFBundleSignature>
    <CFBundleExecutable>laney</CFBundleExecutable>
    <CFBundleIconFile>Assets/Logo/icon.icns</CFBundleIconFile>
    <NSPrincipalClass>NSApplication</NSPrincipalClass>
    <NSHighResolutionCapable>true</NSHighResolutionCapable>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <ApplicationIcon>icon.ico</ApplicationIcon>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>8632</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <NoWarn>8632</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <AvaloniaXaml Remove="Controls\ChatListView.axaml" />
